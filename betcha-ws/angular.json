{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "betcha-app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss", 
          "skipTests": true
        }, 
        "@schematics/angular:service": {
          "skipTests": true
        }, 
        "@schematics/angular:directive": {
          "skipTests": true
        }, 
        "@schematics/angular:pipe": {
          "skipTests": true
        }
      },
      "root": "projects/betcha-app",
      "sourceRoot": "projects/betcha-app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/betcha-app",
            "index": "projects/betcha-app/src/index.html",
            "browser": "projects/betcha-app/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/betcha-app/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/betcha-app/src/favicon.ico",
              "projects/betcha-app/src/assets"
            ],
            "styles": [
              "projects/betcha-app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "prod": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2.5mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "5kb",
                  "maximumError": "8kb"
                }
              ],
              "outputHashing": "all"
            },
            "dev": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2.5mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "5kb",
                  "maximumError": "8kb"
                }
              ],
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/betcha-app/src/environments/environment.ts",
                  "with": "projects/betcha-app/src/environments/environment.dev.ts"
                }
              ]
            }, 
            "stage": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2.5mb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "5kb",
                  "maximumError": "8kb"
                }
              ],
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/betcha-app/src/environments/environment.ts",
                  "with": "projects/betcha-app/src/environments/environment.stage.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "prod"
        },
        "serve": {
          "options": {"port": 4200},
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "betcha-app:build:prod"
            },
            "development": {
              "buildTarget": "betcha-app:build:dev"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "betcha-app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/betcha-app/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/betcha-app/src/favicon.ico",
              "projects/betcha-app/src/assets"
            ],
            "styles": [
              "projects/betcha-app/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
