{
  "name": "betcha-app",
  "version": "0.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration dev",
    "test": "ng test",
    "srv:compile": "npm run build --prefix functions",
    "srv:emulators-seed": "npm run seed-emulators --prefix functions",
    "srv:run": "firebase emulators:start --inspect-functions --export-on-exit em-data --import em-data",
    "srv:kill-ports": "npx kill-port 8080 5001 9099 4300",
    "srv:build": "npm run watch",
    "srv:emulators": "npm run srv:kill-ports && npm run srv:run",
    "srv:concur": "concurrently \"npm:srv:emulators\" \"npm:srv:emulators-seed\"",
    "srv:concur:fb": "concurrently \"npm:srv:emulators\" \"npm:srv:emulators-seed\"",
    "srv": "npm run srv:compile && npm run srv:concur"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.0",
    "@angular/common": "^17.3.0",
    "@angular/compiler": "^17.3.0",
    "@angular/core": "^17.3.0",
    "@angular/fire": "^17.1.0",
    "@angular/forms": "^17.3.0",
    "@angular/platform-browser": "^17.3.0",
    "@angular/platform-browser-dynamic": "^17.3.0",
    "@angular/router": "^17.3.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "^17.3.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^8.2.2",
    "firebase-tools": "^13.10.0",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.4.2"
  }
}